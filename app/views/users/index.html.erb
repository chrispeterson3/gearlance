<h1>Users</h1>

<% if notice %>
  <p><%= notice %></p>
<% end %>

<% @user.each do |user| %>
  <ul>
    <li>
      <p><b>First Name:</b> <%= link_to user.first_name, user_url(user.id) %></p>
      <p><b>Last Name:</b> <%= user.last_name %></p>
      <p><b>Email:</b> <%= user.email %></p>
    </li>
  </ul>
  
<% end %>