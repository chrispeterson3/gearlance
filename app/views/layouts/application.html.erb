<!DOCTYPE html>
<html>
<head>
  <title>Gearlance</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <br>
  <nav>
    <%= link_to "Items", items_url %> | <%= link_to "Categories", categories_url %> | <%= link_to "Users", users_url %>
    <% if signed_in? %> | 
      <%= link_to "My Profile", user_url(session[:user_id]) %> | 
      <%= link_to "Log Out", session_url, method: "delete" %>
    <% else %>
      | <%= link_to "Sign Up", new_user_url %> | 
      <%= link_to "Log In", new_session_url %>
   <% end %>
  </nav>
  <br>

<%= yield %>

</body>
</html>